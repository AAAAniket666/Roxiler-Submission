    "pg-hstore": "^2.3.4",
    "sequelize": "^6.33.0",
    "dotenv": "^16.3.1",
    "helmet": "^7.1.0",
    "express-rate-limit": "^6.10.0",
    "joi": "^17.10.2",
    "express-validator": "^7.0.1",
    "morgan": "^1.10.0",
    "compression": "^1.7.4"
